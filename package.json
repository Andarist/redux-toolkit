{
  "name": "@reduxjs/toolkit",
  "version": "1.5.1",
  "description": "The official, opinionated, batteries-included toolset for efficient Redux development",
  "repository": "https://github.com/reduxjs/redux-toolkit",
  "keywords": [
    "redux",
    "react",
    "starter",
    "toolkit",
    "reducer",
    "slice",
    "immer",
    "immutable"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/reduxjs-toolkit.cjs.js",
  "module": "dist/reduxjs-toolkit.esm.js",
  "umd:main": "dist/reduxjs-toolkit.umd.min.js",
  "unpkg": "dist/reduxjs-toolkit.umd.min.js",
  "types": "dist/declarations/index.ts",
  "author": "Mark Erikson <mark@isquaredsoftware.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.7.5",
    "@babel/preset-typescript": "^7.13.0",
    "@microsoft/api-extractor": "^7.7.0",
    "@preconstruct/cli": "^2.0.6",
    "@types/jest": "^24.0.11",
    "@types/json-stringify-safe": "^5.0.0",
    "@types/nanoid": "^2.1.0",
    "@types/node": "^10.14.4",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "axios": "^0.19.2",
    "console-testing-library": "^0.3.1",
    "eslint-config-react-app": "^5.0.1",
    "invariant": "^2.2.4",
    "json-stringify-safe": "^5.0.1",
    "prettier": "^2.2.1",
    "react": "^16.8.6",
    "tsdx": "^0.11.0",
    "tslib": "^1.10.0",
    "typescript": "^3.9.7",
    "typings-tester": "^0.3.2"
  },
  "scripts": {
    "build-ci": "preconstruct build && api-extractor run",
    "build": "preconstruct build",
    "setup": "preconstruct dev",
    "format": "prettier --write \"src/**/*.ts\" \"**/*.md\"",
    "format:check": "prettier --list-different \"src/**/*.ts\" \"docs/*/**.md\"",
    "lint": "tsdx lint src",
    "prepare": "npm run lint && npm run format:check && npm test && npm run type-tests && npm run build-ci",
    "test": "tsdx test",
    "type-tests": "cd type-tests/files && tsc"
  },
  "files": [
    "src",
    "dist",
    "createSlice"
  ],
  "dependencies": {
    "immer": "^8.0.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  },
  "sideEffects": false,
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "ignoreCodes": [
            6133
          ]
        }
      }
    }
  },
  "preconstruct": {
    "umdName": "RTK",
    "entrypoints": [
      "./index.ts",
      "./createSlice.ts"
    ]
  }
}
